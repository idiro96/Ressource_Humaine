"</div>\n"
"% if ctx.get('tracking'):\n"
"    <ul>\n"
"    % for tracking in ctx['tracking']\n"
"        <li>${tracking[0]} : ${tracking[1]} -&gt; ${tracking[2]}</li>\n"
"    % endfor\n"
"    </ul>\n"
"% endif\n"
